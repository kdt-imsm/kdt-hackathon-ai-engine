{
  "functions": {
    "app/main.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/app/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "$PYTHONPATH:."
    }
  }
}